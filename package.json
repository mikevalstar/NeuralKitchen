{
  "name": "neuralkitchen",
  "version": "1.0.0",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "dev": "concurrently --names \"UI,MCP,TASKS\" --prefix-colors \"blue,green,yellow\" \"pnpm dev:web\" \"pnpm mcp:dev\" \"pnpm tasks:dev\"",
    "dev:no-tasks": "concurrently --names \"UI,MCP\" --prefix-colors \"blue,green\" \"pnpm dev:web\" \"pnpm mcp:dev\"",
    "dev:web": "vite dev",
    "build": "vite build && tsc --noEmit",
    "start": "node .output/server/index.mjs",
    "mcp:dev": "tsx watch src/mcp-server.ts",
    "mcp:start": "tsx src/mcp-server.ts",
    "tasks:dev": "tsx watch src/background-tasks.ts",
    "tasks:start": "tsx src/background-tasks.ts",
    "seed": "tsx prisma/seed.ts",
    "lint": "biome lint .",
    "lint:fix": "biome lint --write .",
    "format": "biome format .",
    "format:fix": "biome format --write .",
    "check": "biome check .",
    "check:fix": "biome check --write .",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:pull": "prisma db pull",
    "db:migrate": "prisma migrate dev",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:migrate:reset": "prisma migrate reset",
    "db:migrate:status": "prisma migrate status",
    "db:studio": "prisma studio",
    "db:seed": "pnpm seed",
    "user:add": "tsx scripts/manage-users.ts add",
    "user:list": "tsx scripts/manage-users.ts list",
    "user:delete": "tsx scripts/manage-users.ts delete"
  },
  "keywords": [],
  "author": "Mike Valstar",
  "license": "MIT",
  "description": "",
